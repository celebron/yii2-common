{
  "name": "celebron/yii2-common",
  "type": "yii2-extension",
  "keywords": ["yii2", "extension"],
  "license": "Apache-2.0",
  "autoload": {
    "psr-4": {
      "Celebron\\common\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Aleksey Shatalin (celebron)",
      "email": "celebron.ru@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "require-dev": {
    "roave/security-advisories": "dev-latest"

  },
  "require": {
    "yiisoft/yii2": "^2.0",
    "php": "^8.2"
  },
  "extra": {
    "branch-alias": {
      "dev-main": "1.0.x-dev"
    }
  },
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    }
  }
}
